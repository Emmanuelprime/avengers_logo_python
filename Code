from turtle import *
bgcolor('black')
penup()
setposition(0, -280)
pendown()
begin_fill()
color('red')
pencolor('white')
circle(300)
end_fill()
penup()
pensize(2)
setposition(0, -230)
pendown()
begin_fill()
color('black')
circle(250)
end_fill()
penup()
setposition(30, -110)
pendown()
begin_fill()
color('red')
pensize(10)
pencolor('white')
forward(23)
backward(123)
left(60)
backward(220)
right(60)
backward(100)
right(117)
backward(710)
right(63)
backward(110)
right(90)
backward(510)
right(90)
backward(100)
right(90)
backward(70)
end_fill()
penup()
pensize(10)
setposition(53, -40)
pendown()
begin_fill()
color('black')
pencolor('white')
right(90)
forward(100)
right(115)
forward(250)
right(157)
forward(227)
end_fill()
backward(80)
left(42)
forward(147)
right(83)
forward(140)
done()
